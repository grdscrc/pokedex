<ul>
    <% @pokemons.each do |pokemon| %>
        <li>
            <%= [pokemon.number, pokemon.name].join(' - ') %>
            <%= link_to edit_pokemon_path(pokemon) do %>
                edit
            <% end %>
        </li>
    <% end %>

    <%= will_paginate @pokemons %>
</ul>
<%= button_to 'New', new_pokemon_path, method: :get %>